<% if user.admin? %>
  <%= link_to update_role_profile_path(user),method: :patch, data: { "turbo-method": :put } do%>
    Upgrade to student
  <%end%>

<% else %>
  <%= link_to update_role_profile_path(user),method: :patch, data: { "turbo-method": :put } do%>
    Upgrade to admin
  <%end%>

<% end %>



